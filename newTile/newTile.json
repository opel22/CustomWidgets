{
    "id": "com.sap.sample.newtile",
    "version": "1.0.0",
    "name": "newTile",
    "description": "A newTile",
    "newInstancePrefix": "newTile",
    "icon": "https://opel22.github.io/CustomWidgets/newTile/icon.png",
    "vendor": "SAP",
    "eula": "",
    "license": "",
    "webcomponents": [
        {
            "kind": "main",
            "tag": "com-sap-sample-newtile",
            "url": "https://opel22.github.io/CustomWidgets/newTile/newTile.js",
            "integrity": "sha256-/HF47ayRHxc4HpYWVmrNoetT6xaC2u5fwbXgUxrDkI8=",
            "ignoreIntegrity": true
        }
    ],
    "properties": {
        "width": {
            "type": "integer",
            "default": 192
        },
        "height": {
            "type": "integer",
            "default": 192
        },
        "value": {
            "type": "string",
            "description": "Tile Value",
            "default": "0.00"
        },
        "name": {
            "type": "string",
            "description": "Tile name"
        }
    },
    "methods": {
        "setValue": {
            "description": "Sets the value.",
            "parameters": [
                {
                    "name": "newValue",
                    "type": "string",
                    "description": "The new Value"
                }
            ],
            "body": "this.value = newValue;"
        },
        "setName": {
            "description": "Sets ID.",
            "parameters": [
                {
                    "name": "newName",
                    "type": "string",
                    "description": "The name of the widget"
                }
            ],
            "body": "this.name = newName;"
        }        
    },
    "events": {
    },
    "imports": ["datasource"]
}            
